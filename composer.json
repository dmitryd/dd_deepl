{
	"name": "dmitryd/dd-deepl",
	"type": "typo3-cms-extension",
	"description": "A TYPO3 CMS extension to translate pages, content and records with DeepL",
	"require": {
		"php": ">=8.2.0,<8.4.0",
		"typo3/cms-core": "^13.4",
		"typo3/cms-backend": "*",
		"deeplcom/deepl-php": "^1.5",
		"phplucidframe/console-table": "^1.2",
		"symfony/console": "^6.4 || ^7.0"
	},
	"conflicts": {
		"deepltranslate": "*",
		"wv_deepltranslate": "*"
	},
	"autoload": {
		"psr-4": {
			"Dmitryd\\DdDeepl\\": "Classes"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "dd_deepl"
		}
	}
}
